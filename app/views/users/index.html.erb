<h1>Listing users</h1>

<table>
  <tr>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Createtime</th>
    <th>Deadtime</th>
    <th>Bomb</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.latitude %></td>
    <td><%= user.longitude %></td>
    <td><%= user.createtime %></td>
    <td><%= user.deadtime %></td>
    <td><%= user.bomb_id %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
